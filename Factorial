import java.io.*;
import java.util.*;

public class Main {

    public static void main(String[] args) throws Exception {
        
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int f = 1;
        System.out.print(factorial(n, f));
        // write your code here
    }

    public static int factorial(int n, int f){
        if(n==1)
        {
        return 1;
        }
        
        f = n * factorial(n-1, f);
        
        return f;
        
        
        
        
    }

}
